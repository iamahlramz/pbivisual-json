{
  "name": "Gauge â€” Reference Theme",
  "visualStyles": {
    "gauge": {
      "*": {
        "general": [{ "responsive": true, "keepLayerOrder": true }],
        "axis": [{ "min": 0, "max": 100 }],
        "target": [{
          "show": true,
          "color": { "solid": { "color": "#252423" } },
          "fontSize": 12,
          "fontFamily": "Segoe UI"
        }],
        "gaugeAxis": [{
          "fill": { "solid": { "color": "#118DFF" } }
