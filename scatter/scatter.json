{
  "name": "Scatter Chart â€” Reference Theme",
  "visualStyles": {
    "scatterChart": {
      "*": {
        "general": [{ "responsive": true, "keepLayerOrder": true }],
        "legend": [{ "show": true, "position": "Top", "showTitle": false, "labelColor": { "solid": { "color": "#605E5C" } }, "fontFamily": "Segoe UI", "fontSize": 9 }],
        "categoryAxis": [{ "show": true, "position": "Bottom", "labelColor": { "solid": { "color": "#605E5C" } }, "fontFamily": "Segoe UI", "fontSize": 9, "showAxisTitle": false, "gridlineShow": true, "gridlineColor": { "solid": { "color": "#E6E6E6" } }, "gridlineStyle": "solid", "gridlineThickness": 1 }],
        "valueAxis": [{ "show": true, "position": "Left", "labelColor": { "solid": { "color": "#605E5C" } }, "fontFamily": "Segoe UI", "fontSize": 9, "showAxisTitle": false, "gridlineShow": true, "gridlineColor": { "solid": { "color": "#E6E6E6" } }, "gridlineStyle": "solid", "gridlineThickness": 1, "start": null, "end": null }],
        "dataPoint": [{ "showAllDataPoints": true }],
        "bubbles": [{ "bubbleSize": -10 }],
        "fillPoint": [{ "show": false }],
        "colorBorder": [{ "show": true }],
        "categoryLabels": [{ "show": false, "color": { "solid": { "color": "#605E5C" } }, "fontFamily": "Segoe UI", "fontSize": 9 }],
        "plotArea": [{ "transparency": 0 }],
        "title": [{ "show": true, "fontColor": { "solid": { "color": "#252423" } }, "background": { "solid": { "color": "#FFFFFF" } }, "alignment": "center", "fontSize": 12, "fontFamily": "DIN", "bold": true }],
        "background": [{ "show": false, "color": { "solid": { "color": "#FFFFFF" } }, "transparency": 0 }],
        "border": [{ "show": false, "color": { "solid": { "color": "#808080" } }, "radius": 0 }]
      }
    }
  }
}
