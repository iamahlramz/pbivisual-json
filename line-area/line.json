{
  "name": "Line Chart â€” Reference Theme",
  "visualStyles": {
    "lineChart": {
      "*": {
        "general": [
          {
            "responsive": true,
            "keepLayerOrder": true
          }
        ],
        "legend": [
          {
            "show": true,
            "position": "Top",
            "showTitle": false,
            "labelColor": { "solid": { "color": "#605E5C" } },
            "fontFamily": "Segoe UI",
            "fontSize": 9
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "position": "Bottom",
            "axisType": "categorical",
            "labelColor": { "solid": { "color": "#605E5C" } },
            "fontFamily": "Segoe UI",
            "fontSize": 9,
            "showAxisTitle": false,
            "gridlineShow": false,
            "gridlineColor": { "solid": { "color": "#E6E6E6" } },
            "gridlineStyle": "solid",
            "gridlineThickness": 1
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "position": "Left",
            "labelColor": { "solid": { "color": "#605E5C" } },
            "fontFamily": "Segoe UI",
            "fontSize": 9,
            "labelDisplayUnits": "0",
            "showAxisTitle": false,
            "gridlineShow": true,
            "gridlineColor": { "solid": { "color": "#E6E6E6" } },
            "gridlineStyle": "solid",
            "gridlineThickness": 1,
            "start": null,
            "end": null
          }
        ],
        "dataPoint": [
          {
            "showAllDataPoints": true
          }
        ],
        "labels": [
          {
            "show": false,
            "color": { "solid": { "color": "#605E5C" } },
            "fontFamily": "Segoe UI",
            "fontSize": 9,
            "labelDisplayUnits": "0",
            "labelPrecision": 1,
            "enableBackground": false,
            "backgroundColor": { "solid": { "color": "#FFFFFF" } }
          }
        ],
        "lineStyles": [
          {
            "strokeWidth": 2,
            "lineStyle": "solid",
            "showMarker": false,
            "markerShape": "circle",
            "markerSize": 5,
            "markerColor": { "solid": { "color": "" } },
            "stepped": false
          }
        ],
        "shapes": [
          {
            "strokeWidth": 2,
            "showMarkers": false,
            "markerSize": 5
          }
        ],
        "plotArea": [
          {
            "transparency": 0
          }
        ],
        "title": [
          {
            "show": true,
            "fontColor": { "solid": { "color": "#252423" } },
            "background": { "solid": { "color": "#FFFFFF" } },
            "alignment": "center",
            "fontSize": 12,
            "fontFamily": "DIN",
            "bold": true
          }
        ],
        "background": [
          {
            "show": false,
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ],
        "border": [
          {
            "show": false,
            "color": { "solid": { "color": "#808080" } },
            "radius": 0
          }
        ]
      }
    }
  }
}
